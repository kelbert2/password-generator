{
	"manifest_version": 2,

	"name": "Password Generator Test",
	"description": "This extension is meant to scramble up easy-to-remember text into a more complicated password",
	"version": 1.0,

	"browser_action": {
		"default_icon":"lock.png",
		//clickable icon
		"default_popup": "popup.html",
		//webpage small popup
		"default_title": "Hello World!"
		// shown when mouse hovers over
	},

	"permissions": [
		"activeTab",
		"https://ajax.googleapis.com/"
	]
}